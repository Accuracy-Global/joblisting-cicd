            <h1 class="text-center mb-5 wow fadeInUp" data-wow-delay="0.1s" style="visibility: visible; animation-delay: 0.1s; animation-name: fadeInUp;">Latest Jobs</h1>
            <div class="row">
                @if (ViewBag.LatestJob1 != null)
                {
                    foreach (LatestJob latestJob in ViewBag.LatestJob1)
                    {
                        <div class="col-sm-3 card" style="margin: 5px; width: 24%; -webkit-animation: slideY 0.6s both; animation: slideY 0.6s both; margin-bottom:25px;">
                            <div class="row" style="padding-bottom:5px;">
                                <div class="col-sm-5" style="padding-top: 10px;">
                                    @Html.Raw(string.Format("<text><span><img  height='100' width='100' src=\"/Image/Avtar/" + latestJob.EmployerId + "\" /><span> </text>"))

                                    @*@if (latestJob.Image != null)
                                        {
                                            @Html.Raw(string.Format("<text><img height='100' width='100' src=\"data:image/jpg;base64," + (latestJob.Image) + " \" /> </text>"));
                                        }
                                        else
                                        {
                                            @Html.Raw(string.Format("<text><img height='100' width='100' src=\"data:image/png;base64,\" alt=\"Image\" /></text>", Url.Content("~/Content/images/non-preview-photo.gif")));
                                        }*@
                                </div>
                                <div class="col-sm-7">
                                    <div class="job-card-title">@Html.Raw(latestJob.Title)</div>
                                    <div class="job-card-subtitle">
                                        <b>Location: </b>@Html.Raw(string.Format("{0} {1}", latestJob.Country, latestJob.City))
                                    </div>
                                    <div class="job-card-subtitle1">
                                        <div class="row" style="padding-top: 0px;width: 100%;margin-top: 6px;">
                                            <div class="col-md-12">
                                                <b>Status: </b> @Html.Raw(latestJob.Status)
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="job-card-buttons">
                                <button class="search-buttons card-buttons detail-btn-override" style="pointer-events: none;">@Html.Raw(latestJob.JobType)</button>
                                @*<button class="search-buttons card-buttons detail-btn-override" style="pointer-events: none;">@Html.Raw(string.Format("{0} ", latestJob.MinimumExperience))</button>*@
