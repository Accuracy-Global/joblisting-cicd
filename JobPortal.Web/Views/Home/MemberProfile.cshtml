@{
    string redirect = string.Empty;
    if (uinfo != null && uinfo.IsJobseeker)
    {
        redirect = Request.QueryString["returnUrl"];
    }
    else
    {
        redirect = Request.QueryString["returnUrl"];
        if (uinfo != null && uinfo.Role != JobPortal.Library.Enumerators.SecurityRoles.Employers)
        {
            redirect = string.Format("/job/preview?returnUrl={0}", Request.QueryString["returnUrl"]);
            if (!string.IsNullOrEmpty(redirect) && !redirect.Contains("indeed"))
            {
                redirect = Request.QueryString["returnUrl"];
            }
        }
    }
}
<script>
    var loggedIn = "@(TempData["LoginStatus"]!=null ? Convert.ToString(TempData["LoginStatus"]) : "")";
    $("#InviteContacts").click(function () {
        $("#inviteDialog").modal("show");
    });

    var authflag = "@Request.IsAuthenticated";

    function loadVerifyDialog() {
        $("#verifyDialog").modal("show");
    }
    function loadCompleteDialog() {
        $("#completeDialog").modal("show");
    }
    $(document).ready(function () {
        if (authflag == "False") {
            $("#joinDialog").modal("show");
        }

        $("#bVerify").click(function () {
            var json = { id: $("#verifyDialog #id").val(), token: $("#verifyDialog #Token").val() };
            var flag = $("#VerifyForm")[0].checkValidity();
            if (flag) {
                $.post("/jsonhelper/confirm", json, function (result) {
                    if (result.Id > 0) {
                        toastr.info(result.Message);
                        $("#verifyDialog").modal("hide");
                        window.location.href = "@redirect";
                    } else {
                        toastr.error(result.Message);
                    }
                });
                return false;
            }
        });

        $("#bChangeEmail").click(function () {
            var json = $("#ChangeEmailForm").serializeObject();
            var flag = $("#ChangeEmailForm")[0].checkValidity();
            if (flag) {
                $.post("/account/changeemail", json, function (result) {
                    if (result.Id > 0) {
                        toastr.info(result.Message);
                        $("#changeEmailDialog #OldEmailAddress").val(json.NewEmailAddress);
                        $("#verifyDialog #eplace").text(json.NewEmailAddress);
                    } else {
                        toastr.error(result.Message);
                    }
                    $("#changeEmailDialog").modal("hide");
                });
                return false;
            }
        });

        $("#verifyDialog .resend").click(function () {
            $.post("/jsonhelper/resend", {}, function (result) {
                toastr.info(result);
            });
        });

        $(".vucsv").click(function () {
            $("#postedFile").val("")
            $("#divvalidFile").html("");
        });

        $(".csvupload").click(function () {
            $("#inviteDialog").modal("hide");
            $("#divvalidFile").html("");
            $("#postedFile").val("");
        });

        $("#photoUpload").click(function () {
            var flag = true;
            if ($("#Photo").val() == null || $("#Photo").val() == "") {
                $("#divPhotoError").html("Please select file!");
                flag = false;
            } else {
                $("#divPhotoError").html("");
            }
            return flag;
        });
        $("#SubmitCSV").click(function () {
            var flag = true;
            if ($("#postedFile").val() == null || $("#postedFile").val() == "") {
                $("#divvalidFile").html("Please select file!");
                flag = false;
            } else {
                $("#divvalidFile").html("");
            }
            return flag;
        });
        $('[data-toggle="tooltip"]').tooltip();
        var cpflag = "@(uinfo != null && uinfo.Type == 4 && uinfo.IsJobseeker == true && weightage < 90)";
        if (cpflag == "True") {
            setTimeout("loadCompleteDialog()", 8000);
        }

        var vdflag = "@(uinfo != null && uinfo.IsConfirmed == false)";
        if (vdflag == "True") {
            loadVerifyDialog();
        }

        var defineSize = 6656;
        var defineSizeText = "";
        var durl = "@ViewBag.DownloadUrl"
        if (durl != "") {
            durl = durl.replace("amp;", "");
            durl = durl.replace("amp;", "");
            window.location.href = "@Request.Url.Scheme://@Request.Url.Authority" + durl;
        }
        var msg = "@message";
        if (msg != null && msg != "") {
            $(".alert").removeClass("in").show();
            $(".alert").delay(10000).addClass("in").fadeOut(10000);
        }
        $("#AddPhotoTopRight").hide();
        $("#AddPhotoTopBottom").hide();
        $("#AddPhotoBackground").hide();
        $("#AddPhoto").hide();

        $("#AddPhoto").click(function () {
            $("#divPhotoError").html("");
            $("#Photo").val("");
            var confirmed = "@member.IsConfirmed";
            defineSize = $(this).data('maxsize');
            defineSizeText = $(this).data('maxsizetext');
            $("#uploadsize").text(defineSizeText);
            if (confirmed == "False") {
                window.location.href = "@Url.Action("ConfirmRegistration","Account")";
            }

            $("#PictureType").val($(this).data('type'));
            var dimension = $(this).data('size');
            $("#imageSize").text(dimension);
        });

        $(".rsbanner").mouseover(function () {
            $("#AddPhotoTopRight").show();
        });

        $(".rsbanner").mouseout(function () {
            $("#AddPhotoTopRight").hide();
        });

        $("#AddPhotoTopRight").click(function () {
            $("#divPhotoError").html("");
            $("#Photo").val("");
            var confirmed = "@member.IsConfirmed";
            defineSize = $(this).data('maxsize');
            defineSizeText = $(this).data('maxsizetext');
            $("#uploadsize").text(defineSizeText);
            if (confirmed == "False") {
                window.location.href = "@Url.Action("ConfirmRegistration","Account")";
            }

            $("#PictureType").val($(this).data('type'));
            var dimension = $(this).data('size');
            $("#imageSize").text(dimension);
            if ($(this).data('type') == "TopRight") {
                var i = $.find("#TopSliderItemList .active");
                var type = $(this).data('type') + i[0].id.replace("Top", "");
                $("#ActivePicture").val(type);
            }
        });

        $(".rsbbom").mouseover(function () {
            $("#AddPhotoTopBottom").show();
        });

        $(".rsbbom").mouseout(function () {
            $("#AddPhotoTopBottom").hide();
        });

        $("#AddPhotoTopBottom").click(function () {
            $("#divPhotoError").html("");
            $("#Photo").val("");
            var confirmed = "@member.IsConfirmed";
            defineSize = $(this).data('maxsize');
            defineSizeText = $(this).data('maxsizetext');
            $("#uploadsize").text(defineSizeText);
            if (confirmed == "False") {
                window.location.href = "@Url.Action("ConfirmRegistration","Account")";
            }

            $("#PictureType").val($(this).data('type'));
            var dimension = $(this).data('size');
            $("#imageSize").text(dimension);

            if ($(this).data('type') == "TopRight") {
                var i = $.find("#TopSliderItemList .active");
                var type = $(this).data('type') + i[1].id.replace("Top", "");
                $("#ActivePicture").val(type);
            }

            if ($(this).data('type') == "BottomRight") {
                var i = $.find(".carousel-inner .active");
                var type = $("#PictureType").val() + i[1].id.replace("Bottom", "");
                $("#ActivePicture").val(type);
            }
        });


        $(".pro-file-sec").mouseover(function () {
            $("#AddPhotoBackground").show();
        });

        $("#AddPhotoBackground").click(function () {
            $("#divPhotoError").html("");
            $("#Photo").val("");
            var confirmed = "@member.IsConfirmed";
            defineSize = $(this).data('maxsize');
            defineSizeText = $(this).data('maxsizetext');
            $("#uploadsize").text(defineSizeText);
            if (confirmed == "False") {
                window.location.href = "@Url.Action("ConfirmRegistration","Account")";
            }

            $("#PictureType").val($(this).data('type'));
            var dimension = $(this).data('size');
            $("#imageSize").text(dimension);

            if ($(this).data('type') == "TopRight") {
                var i = $(".carousel-inner .active").find("img");
                var type = $(this).data('type') + i.attr("id").replace("Top", "");
                $("#ActivePicture").val(type);
            }

            if ($(this).data('type') == "BottomRight") {
                var i = $(".bottom .active").find("img");
                var type = $("#PictureType").val() + i.attr("id").replace("Bottom", "");
                $("#ActivePicture").val(type);
            }
        });

        $(".pro-file-sec").mouseout(function () {
            $("#AddPhotoBackground").hide();
        });

        $(".responsive-circle").mouseover(function () {
            $("#AddPhoto").show();
        });
        $(".responsive-circle").mouseout(function () {
            $("#AddPhoto").hide();
        });

        $(".pro-file-sec").dblclick(function () {
            $(".photolist .item").removeClass("active");
            $("#imgBackground").addClass("active");
            $("#photoGallery").modal("show");
        });

        $(".pro-file-sec").on('doubletap', function () { // Mobile browsers only
            $(".photolist .item").removeClass("active");
            $("#imgBackground").addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#TopFirst").on('doubletap', function () {
            var type = "BottomFirst";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#TopSecond").on('doubletap', function () {
            var type = "TopSecond";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#TopFirst").dblclick(function () {
            var type = "TopFirst";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#TopSecond").dblclick(function () {
            var type = "TopSecond";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });


        $("#BottomFirst").on('doubletap', function () {
            var type = "BottomFirst";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#BottomSecond").on('doubletap', function () {
            var type = "BottomSecond";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#BottomFirst").dblclick(function () {
            var type = "BottomFirst";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        $("#BottomSecond").dblclick(function () {
            var type = "BottomSecond";
            $(".photolist .item").removeClass("active");
            $("#img" + type).addClass("active");
            $("#photoGallery").modal("show");
        });

        var imgSize = "";

        $("#Photo").change(function () {
            var file = document.getElementById('Photo').files[0];

            var fileSize;
            if (file.size == undefined) {
                fileSize = file.fileSize;
            } else {
                fileSize = file.size
            }
            var name = file.name;
            var ext = name.substr(name.lastIndexOf(".") + 1, 4).toLowerCase();
            var allowed = "png, jpg, jpeg, gif";
            $("#divPhotoError").html("");
            if (name.length <= 0) {
                $("#divPhotoError").html("Please select file!");
                $("#photoUpload").attr('disabled', true);
            }

            if (fileSize > (defineSize * 1024)) {
                $("#divPhotoError").html("Up to " + defineSizeText + " max file size.!");
                $("#photoUpload").attr('disabled', true);
            } else {
                $("#photoUpload").attr('disabled', false);
            }

            if (allowed.indexOf(ext) < 0) {
                $("#divPhotoError").html("Allowed file type are JPG, JPEG, PNG, GIF only.!");
                $("#photoUpload").attr('disabled', true);
            } else {
                $("#photoUpload").attr('disabled', false);
            }

            var _URL = window.URL || window.webkitURL;
            var image = new Image();

            image.onload = function () {
                //alert("The image width is " + this.width + " and image height is " + this.height);
                if (this.height < this.width && this.height < 135) {
                    imgSize = "contain";
                } else {
                    imgSize = "cover";
                }
            };
            image.src = _URL.createObjectURL(file);
        });

        $("#photoUpload").click(function () {
            $("#divAjaxBar").show();
            var file = document.getElementById('Photo').files[0];
            var name = file.name;
            var ext = name.substr(name.lastIndexOf(".") + 1, 4).toLowerCase();

            var reader = new FileReader();
            reader.readAsDataURL(file);

            reader.onload = function (e) {
                var binaryString = e.target.result.toString();
                binaryString = binaryString.replace("data:image/png;base64,", "");
                binaryString = binaryString.replace("data:image/jpeg;base64,", "");
                binaryString = binaryString.replace("data:image/gif;base64,", "");
                binaryString = binaryString.replace("data:image/jpg;base64,", "");
                var type = "";
                if (ext == "jpg") {
                    type = "data:image/jpg;base64";
                } else if (ext == "jpeg") {
                    type = "data:image/jpeg;base64";
                } else if (ext == "png") {
                    type = "data:image/png;base64";
                } else if (ext == "gif") {
                    type = "data:image/gif;base64";
                }

                var loc = $("#PictureType").val();
                if (loc == "TopRight") {
                    loc = $("#ActivePicture").val();
                }

                if (loc == "BottomRight") {
                    loc = $("#ActivePicture").val();
                }

                $.post('@Url.Action("PictureUpload", "JsonHelper")', { Username: "@(User != null ? username : "")", data: binaryString, area: loc, type: type, size: imgSize },
                   function (result) {
                       $("#alertMessage").text(result);
                       $(".alert").removeClass("in").show();
                       $(".alert").delay(5000).addClass("in").fadeOut(5000);

                   });
            }; 28
        });

        if (loggedIn != "") {
            setTimeout("showInfoBox()", 17000);
        }

        $(".mBlock").click(function () {
            var bname = $(this).data('name');
            var actionUrl = $(this).data('href');
            var loggedInUserRole = "@(Request.IsAuthenticated ? uinfo.Role.GetDescription() : "")";
            var msgText = "";
            var role = $(this).data('role');
            var isConfirmed = "@(Request.IsAuthenticated ? uinfo.IsConfirmed : false)";
            if (isConfirmed == "True") {
                if ((role == "Employers" && loggedInUserRole == "Jobseeker") || (loggedInUserRole == "Employers" && role == "Jobseeker")) {
                    var msgText = "<b>Following may be withdrawn with <span id=\"bname\" style=\"font-weight:bold\">" + bname + "</span>:</b><br/>";
                    msgText += "<ul>";
                    msgText += "<li>Application(s)!</li>";
                    msgText += "<li>Matching!</li>";
                    msgText += "<li>Bookmark(s)!</li>";
                    msgText += "<li>Interview(s)!</li>";
                    msgText += "<li>Connection!</li>";
                    msgText += "</ul>";
                } else {
                    if ($(this).data('connected') == "True") {
                        msgText = "<b>Block</b> action will disconnect you with <span id=\"bname\" style=\"font-weight:bold\"><b>" + bname + "</span></b><br/>";
                    } else {
                        msgText = "<b>Block</b> <span id=\"bname\" style=\"font-weight:bold\"><b>" + bname + "</span></b><br/>";
                    }
                }
                msgText += "<div style=\"text-align:right; color:red; font-size:1.5em;\">Are you sure?</div>";
                $(".modal-body #divMsg").html(msgText);

                actionUrl = $(this).data('href');
                $("#dialogOk").click(function () {
                    window.location.href = actionUrl;
                });
            }
        });
        var flag = "@Request.IsAuthenticated";
        if (flag == "True") {
            $(".webpart").each(function (index, item) {
                var url = "@Request.Url.Scheme://@Request.Url.Authority" + $(item).data("url");
                if (url && url.length > 0) {
                    $(item).load(url);
                }
            });
            setInterval("load_people()", 30000);
            setInterval("load_viewed()", 30000);
        }
    });

    function load_connections() {
        $("#ConnectionList").each(function (index, item) {
            var url = "@Request.Url.Scheme://@Request.Url.Authority" + $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });
    }

    function load_people() {
        $("#PeopleList").each(function (index, item) {
            var url = "@Request.Url.Scheme://@Request.Url.Authority" + $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });
    }

    function load_viewed() {
        $("#PeopleList").each(function (index, item) {
            var url = "@Request.Url.Scheme://@Request.Url.Authority" + $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });
    }

    function showInfoBox() {
        $('[data-toggle="popover"]').popover({
            template:
                '<div class="popover">\
<div class="arrow"></div>\
<div class="popover-header">\
<button type="button" class="close" data-dismiss="popover" aria-hidden="true">&times;</button>\
<h3 class="popover-title"></h3>\
</div>\
<div class="popover-content"></div>\
</div>'
        });
        $('[data-toggle="popover"]').popover("show");
        $(".popover").click(function () {
            $('[data-toggle="popover"]').popover("hide");
        });
        setTimeout("hideInfoBox()", 12000);
    }
    function hideInfoBox() {
        $('[data-toggle="popover"]').popover("hide");
    }
</script>