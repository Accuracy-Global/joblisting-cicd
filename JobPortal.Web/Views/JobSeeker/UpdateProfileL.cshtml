            var eid = '#' + $(this).data('field') + 'Editor > .chars_left';
            $(eid).html("<b style=\"color:#000\">Remaining characters:</b> " + (max_chars - chars_without_html));
            alarmChars();
        });

        $(".editor").mouseenter(function () {
            max_chars = $(this).data("char");
