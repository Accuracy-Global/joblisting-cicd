
@{
    ViewBag.Title = "Payment Process - Failed";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="title-login">
            <h1 style="margin-top:-15px;">Payment Failed</h1>
        </div>
    </div>
</div>
<div class="text-center">
    @Html.Partial("_MessagePartial")
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
       &nbsp;
    </div>
</div>
<div class="text-center">
    <button id="Back" class="btn btn-info btn-block">Back</button>
</div>
<script>
    $(document).ready(function () {
        $("#Back").click(function () {
            window.history.back();
        });
    });
</script>