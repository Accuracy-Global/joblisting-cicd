@model IEnumerable<JobPortal.Web.Models.ActivityStreamModel>
@using JobPortal.Data
@using JobPortal.Domain
@using JobPortal.Web.Models
@using JobPortal.Library.Enumerators
@using JobPortal.Library.Helpers
@{
    Layout = null;
    UserInfo defaultUserInfo = ViewBag.Default;
    UserProfile member = ViewBag.Member;
    string username = User.Username;
    string links = string.Empty;

    if (!Request.IsAuthenticated)
    {
        links += "<a href=\"javascript:void();\"  style=\"color:#aaaaaa\">Connect</a> | ";
        links += "<a href=\"javascript:void();\"  style=\"color:#aaaaaa\">Message</a> | ";
        links += "<a href=\"javascript:void();\"  style=\"color:#aaaaaa\">Share</a> | ";
        links += "<a href=\"javascript:void();\"  style=\"color:#aaaaaa\">Download</a> | ";
    }
    else
    {
        if (!string.IsNullOrEmpty(username))
        {
            if (username != member.Username)
            {
                if (!defaultUserInfo.IsConnected)
                {
                    links += string.Format("<a href=\"{0}\" target=\"_blank\">Connect</a> | ", defaultUserInfo.ConnectUrl);
                }
                links += string.Format("<a href=\"{0}\" target=\"_blank\">Message</a> | ", defaultUserInfo.MessageUrl);
            }
        }
        else
        {
            links += string.Format("<a href=\"{0}\" target=\"_blank\">Connect</a> | ", defaultUserInfo.ConnectUrl);
            links += string.Format("<a href=\"{0}\" target=\"_blank\">Message</a> | ", defaultUserInfo.MessageUrl);
        }
        links += string.Format("<a href=\"{0}\" target=\"_blank\">Share</a>", defaultUserInfo.ShareUrl);
        if (!string.IsNullOrEmpty(defaultUserInfo.Content) && defaultUserInfo.CategoryId != null && defaultUserInfo.SpecializationId != null)
        {
            links += string.Format(" | <a href=\"{0}\" target=\"_blank\">Download</a>", defaultUserInfo.DownloadUrl);
        }
        if (username != member.Username)
        {
            if (!defaultUserInfo.IsBlockedByMe)
            {
                links += string.Format("{0}", defaultUserInfo.BlockLink);
            }
            else
            {
                links += string.Format(" | <a href=\"{0}\" target=\"_blank\">Unblock</a>", defaultUserInfo.UnblockUrl);
            }
        }
    }
}
