@using System.Text
@using JobPortal.Web
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="google-site-verification" content="gLBvu4VbMCPVV7qjMkEibKUpERJGFPxaWN7udQg8Ztc" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=2.0, user-scalable=yes">
    <link rel="icon" href="~/favicon.png" type="image/png">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <style type="text/css">        
        body {
            width: 100%;
            height: 100%;
            overflow-x:hidden;
        }

        .bday li, .footmenu li, .headmenu li {
            display: inline-block;
        }

        .soc {
            display: block;
            font-size: 0;
            list-style: none;
            margin: 0;
        }

            .soc, .soc em {
                text-align: center;
            }

                .mstyle, .soc em {
                    bottom: 0;
                    left: 0;
                }

                .soc li {
                    display: inline-block;
                    margin-left: 3px;
                    /*margin-right: 3px;*/
                }

                .soc button, .soc svg {
                    display: block;
                }

                .soc button {
                    position: relative;
                    height: 34px;
                    width: 34px;
                    padding: 5px;
                    border-radius: 100%;
                    border: 2px solid #fff;
                    cursor: pointer;
                    font-weight: bold;
                }

                .soc svg {
                    height: 100%;
                    width: 100%;
                }

                .soc em {
                    font-size: 14px;
                    line-height: 1.5;
                    margin-top: -.75em;
                    position: absolute;
                    top: 50%;
                    right: 0;
                }

        .icon-10 {
            color: #fff;
            fill: #fff;
            background: #4060a5;
            border: 2px solid #4060a5;
        }

            .icon-10:hover {
                background-color: #fff;
                color: #4060a5;
                fill: #4060a5;
                border: 2px solid #4060a5;
            }

        .icon-14 {
            color: #fff;
            fill: #fff;
            background: #e64522;
            border: 2px solid #e64522;
        }

            .icon-14:hover {
                background-color: #fff;
                color: #e64522;
                fill: #e64522;
                border-color: #e64522;
            }

        .icon-17 {
            color: #fff;
            fill: #fff;
            background-color: #0094bc;
            border: 2px solid #0094bc;
        }

            .icon-17:hover {
                background-color: #fff;
                color: #0094bc;
                fill: #0094bc;
                border-color: #0094bc;
            }

        .icon-26 {
            color: #fff;
            fill: #fff;
            background-color: #00abe3;
            border: 2px solid #00abe3;
        }

            .icon-26:hover {
                background-color: #fff;
                color: #00abe3;
                fill: #00abe3;
                border-color: #00abe3;
            }


        .mstyle {
            position: fixed;
            top: 0;
            right: 0;
            overflow: hidden;
        }

        body > div > div.left {
            position: absolute;
            top: 0;
            left: 0;
            right: 645px;
            height: 100%;
            background-image: url(data:image/png;base64,);
            background-repeat: no-repeat;
            background-position: center;
            background-size: contain;
            background-color: #2789C4;
        }

        body > div > div.right {
            position: absolute;
            right: 0;
            width: 645px;
            height: 100%;
            background-color: #fff;
            padding: 25px 100px 10px 35px;
            overflow: auto;
        }

        .footer {
            position: fixed;
            right: 0;
            bottom: 0;
            height: 40px;
            width: 645px;
            background: #fff;
            padding: 0 25px 10px 35px;
        }

        .footmenu, .headmenu {
            list-style-type: none;
            padding-left: 0;
        }

        .headmenu {
            font-size: 1.25em;
        }

            .headmenu li {
                padding-right: 10px;
            }

        .footmenu {
            font-weight: lighter;
            font-size: 1em;
        }

            .footmenu li {
                padding-right: 5px;
            }

        @@media screen and (max-width:645px) {
            body, body > div > div.right, html {
                height: 100%;
                width: 100%;
            }

                .footer, body > div > div.right {
                    position: fixed;
                    right: 0;
                    width: 100%;
                }

                body > div > div.right {
                    background-color: #fff;
                    padding: 25px 15px 10px;
                    overflow: auto;
                }

            .footer {
                bottom: 0;
                height: 40px;
                background: #fff;
                padding: 10px 25px 10px 15px;
            }

            .footmenu {
                font-weight: lighter;
                font-size: .95em;
            }
        }

        .bday {
            padding-left: 0;
            list-style-type: none;
        }
    </style>
</head>
<body>
    <div class="mstyle">
        <div class="left">
        </div>
        <div class="right">
            <div id="rightContainer">
                <img src="data:image/png;base64," alt="Joblisting" border="0" />
                <div class="clear" style="height:15px;"></div>
                <ul class="headmenu">
                    <li>
                        @Html.ActionLink("Search Jobs", "SearchJobs", "Job")
                    </li>
                    <li>
                        @Html.ActionLink("Search People", "SearchPeople", "Home")
                    </li>                   
                    <li>
                        @Html.ActionLink("Search Jobseekers", "Search", "Jobseeker")
                    </li>
                    <li>@Html.ActionLink("List Job", "ListJob", "Employer")</li>
                </ul>
                @RenderBody()
                <div class="clear" style="height:15px;"></div>
            </div>
            <footer id="loginFooter" class="footer">
                <div style="font-size:small">
                    <ul class="footmenu">
                        <li>U.S. Patent Pending</li>
                        <li>
                            <a href="/copyright" target="_blank" style="color:#808080">Copyright Joblisting</a>
                        </li>
                        <li>
                            <a href="/terms" target="_blank" style="color:#000">Terms Of Use</a>
                        </li>
                        <li>
                            <a href="/privacy" target="_blank" style="color:#000">Privacy</a>
                        </li>
                    </ul>
                </div>
            </footer>
        </div>
    </div>
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-58556403-1', 'auto');
        ga('send', 'pageview');

    </script>

    <script type="text/javascript">
        var ag, re, un, fn, ln, pw, cp, cn, rf, id, cd, co, bd, bm, by, ge, ed, mg, ty;
        function rginit() {
            ag = document.getElementById("Agreed");
            re = document.getElementById("Register");
            un = document.getElementById("Username");
            fn = document.getElementById("FirstName");
            ln = document.getElementById("LastName");
            pw = document.getElementById("Password");
            cp = document.getElementById("ConfirmPassword");
            cn = document.getElementById("Company");
            id = document.getElementById("divIndividual");
            cd = document.getElementById("divCompany");
            co = document.getElementById("CountryId");
            bd = document.getElementById("BDay");
            bm = document.getElementById("BMonth");
            by = document.getElementById("BYear");
            ge = document.getElementById("Gender");
            ed = document.getElementById("divError");
            re.disabled = false;
            un.focus();
            id.setAttribute("class", "hide");
            cd.setAttribute("class", "hide");
            ed.setAttribute("class", "hide");
            ty = document.getElementById("AccountType");

            if (ty.value == 5) {
                cd.setAttribute("class", "show");
                id.setAttribute("class", "hide");
            } else if (ty.value == 4) {
                cd.setAttribute("class", "hide");
                id.setAttribute("class", "show");
            }

            ty.onchange = function () {
                if (ty.value == 5) {
                    cd.setAttribute("class", "show");
                    id.setAttribute("class", "hide");
                } else if (ty.value == 4) {
                    cd.setAttribute("class", "hide");
                    id.setAttribute("class", "show");
                } else {
                    cd.setAttribute("class", "hide");
                    id.setAttribute("class", "hide");
                }
            }

            re.onclick = function () {
                return vd();
            }

            ag.onchange = function () {
                var e = ag.checked;
                re.disabled = !e
            }
        }

        // check valid email address
        function ve(e) {
            var t = /^(([^<>()\[\]\\.,;:\s@@"]+(\.[^<>()\[\]\\.,;:\s@@"]+)*)|(".+"))@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return t.test(e);
        }
        function vepwd(e) {
            var t = /(?=.*\d)(?=.*[a-zA-Z]).{9,}/;
            return t.test(e);
        }
        // checks form validity
        function vd() {
            var f = false;
            mg = "";
            un = document.getElementById("Username");
            fn = document.getElementById("FirstName");
            ln = document.getElementById("LastName");
            pw = document.getElementById("Password");
            cp = document.getElementById("ConfirmPassword");
            cn = document.getElementById("Company");
            co = document.getElementById("CountryId");
            ty = document.getElementById("AccountType");
            bd = document.getElementById("BDay");
            bm = document.getElementById("BMonth");
            by = document.getElementById("BYear");
            ge = document.getElementById("Gender");
            ed = document.getElementById("divError");

            if (un.value.trim() == "") {
                mg += "Email Address should not be empty!<br/>";
            } else if (!ve(un.value.trim())) {
                mg += "Email Address should be in correct format!<br/>";
            }

            if (fn.value.trim() == "") {
                mg += "First Name should not be empty!<br/>";
            }

            if (ln.value.trim() == "") {
                mg += "Last Name should not be empty!<br/>";
            }

            if (pw.value.trim() == "") {
                mg += "Empty password or space in password not allowed!<br/>";
            }

            if (pw.value.trim().length < 5) {
                mg += "Password Min. 9 numbers & letters mixed!<br/>";
            } else if (!vepwd(pw.value.trim())) {
                mg += "Password Min. 9 numbers & letters mixed!<br/>";
            }

            if (cp.value.trim() == "") {
                mg += "Confirm Password should not be empty!<br/>";
            }

            if (pw.value != cp.value) {
                mg += "Password must match with Confirm Password!<br/>";
            }

            if (co.value == "") {
                mg += "Select country!<br/>";
            }

            if (ty.value == "") {
                mg += "Select account type!<br/>";
            }

            if (ty.value == 4) {
                if (ge.value == "") {
                    mg += "Select Gender!<br/>";
                }
                if (by.value == "") {
                    mg += "Select birth date!";
                }
            } else if (ty.value == 5) {
                if (cn.value == "") {
                    mg += "Company should not be empty!<br/>";
                }
                if (cn.value.length < 5) {
                    mg += "Company Minimum 5 characters!<br/>";
                }
            }

            if (mg != "") {
                ed.setAttribute("class", "hide");
                ed.innerHTML = '<div class="message-error" style="font-size:1em; font-weight:normal">' + mg + '</div><div class="clear" style="height:15px"></div>'
                ed.setAttribute("class", "show");
                f = false;
            } else {
                ed.setAttribute("class", "hide");
                f = true;
            }
            return f;
        }
        rginit();

        $(function () {
            $("#Username").focus();
            $("#Register").attr("disabled", true);
            $.getJSON("/help/countrylist", {}, function (list) {
                $("#CountryId").empty();
                $("#CountryId").append("<option value = ''>SELECT COUNTRY</option>");

                if (list.length != 0) {
                    $(list).each(function () {
                        $("#CountryId").append("<option value =" + this.Id + ">" + this.Name + "</option>");
                    });
                }
            });
        });
    </script>
    <script>
        var lgu, lgp, led;        
        function vdl() {
            debugger;
            var f = false;
            lmg = "";
            lgu = $("#LoginUsername");
            lgp = $("#LoginPassword");
            led = $("#divErrorBx");

            if (lgu.val().trim() == "") {
                lmg += "Provide Email Address!<br/>";
            } else if (!ve(lgu.val().trim())) {
                lmg += "Provide Email Address in correct format!<br/>";
            }           

            if (lgp.val().trim() == "") {
                lmg += "Empty password or space in password not allowed!<br/>";
            }

            if (lmg != "") {
                led.hide();
                led.html('<div class="message-error" style="font-size:1em; font-weight:normal">' + lmg + '</div><div class="clear" style="height:15px"></div>');
                led.show();
                f = false;
            } else {
                led.hide();
                f = true;
            }
            return f;
        }

        $(document).ready(function () {
            $("#divErrorBx").hide();
            $("#LoginUsername").change(function () {
                $("#LoginUsername").val($("#LoginUsername").val().trim());
            });

            $("#login").click(function () {
                return vdl();
            });
        });    
    </script>    
</body>
</html>