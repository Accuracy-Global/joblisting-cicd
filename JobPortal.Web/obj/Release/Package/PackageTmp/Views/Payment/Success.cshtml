@{
    ViewBag.Title = "Payment Process - Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        &nbsp;
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
        <img src="data:image/png;base64," style="max-width:100px; max-height:100%; min-width:80px; min-height:80px; width:100%; height:100%;" />
    </div>
</div>
<div class="text-center">
    <h1>Thank you!</h1>
    <div style="font-size:24px; font-weight:lighter">Payment completed successfully.</div>
    <div style="font-size:24px; font-weight:lighter">You should receive a confirmation email shortly.</div>
</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        &nbsp;
    </div>
</div>
<script>
    var durl = "@(ViewBag.ReturnUrl!=null ? ViewBag.ReturnUrl : "")";
    $(document).ready(function () {
        if (durl != "") {
            setTimeout("loadurl()", 2000);
        }
    })

    function loadurl() {
        durl = durl.replace("&amp;", "&");
        console.log(durl);
        if (durl.indexOf("Download") > -1) {
            window.open(durl, "_blank");
            window.location.href = "/downloads";            
        } else {
            window.location.href = durl;
        }
    }
</script>