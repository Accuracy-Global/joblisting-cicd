<script type="text/javascript">

    var rows = 0;
    var pages = 1;
    var offset = 1;

    var pjOffset = 0;
    var pjPages = 1;
    var formId;
    var authenticated = "@Request.IsAuthenticated";
    var countryId = Number("@(Request.QueryString["CountryId"] != null ? Request.QueryString["CountryId"] : "0")");
    function DisplayPromotedProfiles() {
        var jsonForm = formId.serializeObject();
        if (pjOffset < pjPages) {
            pjOffset = pjOffset + 1;
        } else {
            pjOffset = 1;
        }

        jsonForm.PageNumber = pjOffset;

        $.post("/jobseeker/PromotedJobseekers", jsonForm, function (result) {
            $("#PaidResultArea").empty();
            if (result.Data.length > 0) {
                $("#noresult").hide();
                var rows = result.Data[0].MaxRows;
                pjPages = Math.ceil(rows / 2);
                $(result.Data).each(function () {
                    this.authflag = "@Request.IsAuthenticated";
                    this.role = "@(user!= null?user.Role.GetDescription():"")"
                    $("#PaidResultItem").tmpl(this).appendTo("#PaidResultArea");
                });
            }
        });
    }

    $(document).ready(function () {

        if (countryId > 0) {
            $("#waitDialog").modal("show");
            $("#noresult").hide();
            var jsonForm = $(this).closest("form").serializeObject();
            formId = $(this).closest("form");
            jsonForm.CountryId = countryId;
            $("#PaidArea").removeClass("hide");
            $("#PaidArea").addClass("show");
            DisplayPromotedProfiles();
            setInterval("DisplayPromotedProfiles()", 10000);

            $.post("/jobseeker/search", jsonForm, function (result) {
                $("#waitDialog").modal("hide");
                var data_list = result.Data.ModelList;
                if (data_list.length > 0) {
                    $("#noresult").hide();
                    rows = data_list[0].MaxRows;
                    pages = Math.ceil(rows / 10);
                    $("#DisplayArea").removeClass("hide");
                    $("#DisplayArea").addClass("show");
                    $("#ResultArea").empty();
                    $(data_list).each(function () {
                        this.authflag = "@Request.IsAuthenticated";
                        this.role = "@(user!= null?user.Role.GetDescription():"")";
                        $("#ResultItem").tmpl(this).appendTo("#ResultArea");
                    });
                } else {
                    $("#noresult").show();
                    $("#DisplayArea").removeClass("show");
                    $("#DisplayArea").addClass("hide");
                }

                if ((offset < pages)) {
                    if (authenticated == "True") {
                        $("#JoinSeeMore").removeClass("show");
                        $("#JoinSeeMore").addClass("hide");
                        $("#ViewMore").removeClass("hide");
                        $("#ViewMore").addClass("show");

                    } else {
                        $("#JoinSeeMore").removeClass("hide");
                        $("#JoinSeeMore").addClass("show");
                        $("#ViewMore").removeClass("show");
                        $("#ViewMore").addClass("hide");
                    }
                } else {
                    $("#JoinSeeMore").removeClass("show");
                    $("#JoinSeeMore").addClass("hide");
                    $("#ViewMore").removeClass("show");
                    $("#ViewMore").addClass("hide");
                }
            });
        }

        $(".search").click(function () {
            $("#waitDialog").modal("show");
            $("#noresult").hide();
            var jsonForm = $(this).closest("form").serializeObject();
            formId = $(this).closest("form");

            $("#PaidArea").removeClass("hide");
            $("#PaidArea").addClass("show");
            DisplayPromotedProfiles();
            setInterval("DisplayPromotedProfiles()", 10000);

            $.post("/jobseeker/search", jsonForm, function (result) {
                $("#waitDialog").modal("hide");
                var data_list = result.Data.ModelList;
                if (data_list.length > 0) {
                    rows = data_list[0].MaxRows;
                    pages = Math.ceil(rows / 10);
                    $("#DisplayArea").removeClass("hide");
                    $("#DisplayArea").addClass("show");
                    $("#ResultArea").empty();
                    $("#noresult").hide();
                    $(data_list).each(function () {
                        this.authflag = "@Request.IsAuthenticated";
                        this.role = "@(user!= null?user.Role.GetDescription():"")";
                        $("#ResultItem").tmpl(this).appendTo("#ResultArea");
                    });
                } else {
                    $("#noresult").show();
                    $("#DisplayArea").removeClass("show");
                    $("#DisplayArea").addClass("hide");
                }

                if ((offset < pages)) {
                    if (authenticated == "True") {
                        $("#JoinSeeMore").removeClass("show");
                        $("#JoinSeeMore").addClass("hide");
                        $("#ViewMore").removeClass("hide");
                        $("#ViewMore").addClass("show");

                    } else {
                        $("#JoinSeeMore").removeClass("hide");
                        $("#JoinSeeMore").addClass("show");
                        $("#ViewMore").removeClass("show");
                        $("#ViewMore").addClass("hide");
                    }
                } else {
                    $("#JoinSeeMore").removeClass("show");
                    $("#JoinSeeMore").addClass("hide");
                    $("#ViewMore").removeClass("show");
                    $("#ViewMore").addClass("hide");
                }
            });

            return false;
        });

        var sender = "@(Request.UrlReferrer != null ? Request.UrlReferrer.GetLeftPart(UriPartial.Path) : string.Empty)";
        var current = "@Request.Url.GetLeftPart(UriPartial.Path)";

        if (current == sender) {
            var flag = $.cookie('searchjobseeker');
            if (flag == 'advance') {
                $('#AdvanceSearch').show();
                $('#BasicSearch').hide();
            } else if (flag == 'basic') {
                $('#AdvanceSearch').hide();
                $('#BasicSearch').show();
            }
        }

        $("#AdvanceSearchButton").click(function () {
            $.cookie('searchjobseeker', 'advance');
            $('#AdvanceSearch').show();
            $('#BasicSearch').hide();
            $("#Title").val("");
            $("#Where").val("");
            return false;
        })

        $("#BasicSearchButton").click(function () {
            $.cookie('searchjobseeker', 'basic');
            $('#AdvanceSearch').hide();
            $('#BasicSearch').show();

            $("#Title").val("");
            $("#CategoryId").val("");
            $("#SpecializationId").val("");
            $("#CountryId").val("");
            $("#StateId").val("");
            $("#City").val("");
            $("#AgeMin").val("");
            $("#AgeMax").val("");
            $("#QualificationId").val("");
            $("#Experience").val("");
            $("#SalaryMin").val("");
            $("#SalaryMax").val("");
            return false;
        })

        $("#ViewMore").click(function () {
            $("#waitDialog").modal("show");

            var jsonForm = formId.serializeObject();
            if (offset < pages) {
                offset = offset + 1;
            }
            jsonForm.PageNumber = offset;
            $.post("/jobseeker/search", jsonForm, function (result) {
                $("#waitDialog").modal("hide");
                var data_list = result.Data.ModelList;
                if (data_list.length > 0) {
                    $("#noresult").hide();
                    $(data_list).each(function () {
                        this.authflag = "@Request.IsAuthenticated";
                        this.role = "@(user!= null?user.Role.GetDescription():"")";
                        this.authenticated = "@Request.IsAuthenticated";
                        $("#ResultItem").tmpl(this).appendTo("#ResultArea");
                    });

                    if (offset < pages) {
                        if (authenticated == "True") {
                            $("#JoinSeeMore").removeClass("show");
                            $("#JoinSeeMore").addClass("hide");
                            $("#ViewMore").removeClass("hide");
                            $("#ViewMore").addClass("show");
                        } else {
                            $("#JoinSeeMore").removeClass("hide");
                            $("#JoinSeeMore").addClass("show");
                            $("#ViewMore").removeClass("show");
                            $("#ViewMore").addClass("hide");
                        }
                    } else {
                        $("#JoinSeeMore").removeClass("show");
                        $("#JoinSeeMore").addClass("hide");
                        $("#ViewMore").removeClass("show");
                        $("#ViewMore").addClass("hide");
                    }

                } else {
                    $("#ViewMore").removeClass("show");
                    $("#ViewMore").addClass("hide");
                    $("#noresult").show();
                }
            });
            return false;
        });

        $("#JoinSeeMore").click(function () {
            var jsonForm = $(formId).serializeObject();
            console.log(jsonForm);
            var qs = "@Request.Url.ToString()?" + $.param(jsonForm);
            var url = "/account/register?returnurl=" + encodeURIComponent(qs);
            window.location.href = url;
        });
    });
</script>

